(program
 (def
  (binary_search a x lo hi)
  (suite
   (cond ((comparison hi (is None)) (suite (= (hi) (indexed len (called a))))))
   (while
    (comparison lo ("<" hi))
    (suite
     (= (mid) (term (arith lo ("+" hi)) ("//" 2)))
     (= (midval) (indexed a (subscript mid)))
     (cond
      ((comparison midval ("<" x)) (suite (= (lo) (arith mid ("+" 1)))))
      ((comparison midval (">" x)) (suite (= (hi) mid)))
      (else (suite (return mid))))))
   (return ("-" 1))))
 (expr
  (indexed
   print
   (called
    (indexed binary_search (called (list 1 3 4 5 6 7 9 10) 3 0 None))))))

